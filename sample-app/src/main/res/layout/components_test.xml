<?xml version="1.0" encoding="UTF-8"?>
<!-- Generated from components_test.json with Data Binding -->
<!-- DO NOT EDIT MANUALLY - Use 'kjui generate' to update -->
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <import type="android.view.View"/>
        <variable
            name="data"
            type="com.example.kotlinjsonui.sample.data.ComponentsTestData"/>
        <variable
            name="viewModel"
            type="com.example.kotlinjsonui.sample.viewmodels.ComponentsTestViewModel"/>
    </data>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="20dp">
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:textColor="#FFFFFF"
                android:text="@string/dynamic_dynamicmodestatus"
                android:onClick="toggleDynamicMode"
                android:background="@drawable/ripple_b39393a0"
                android:textSize="14sp"
                android:textStyle="bold"
                android:padding="8dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/new_components_test"
                android:textSize="24sp"
                android:textColor="#333333"
                android:layout_margin="0dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/togglecheckbox_components"
                android:textSize="18sp"
                android:textColor="#666666"
                android:layout_margin="0dp"/>
            <View
                android:id="@+id/toggle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/enable_notifications"
                android:layout_margin="0dp"/>
            <CheckBox
                android:id="@+id/checkbox1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/i_agree_to_terms"
                android:layout_margin="0dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/progress_slider"
                android:textSize="18sp"
                android:textColor="#666666"
                android:layout_margin="0dp"/>
            <View
                android:id="@+id/progress1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/download_progress"
                android:progress="@{data.progress1Value}"
                android:layout_margin="0dp"/>
            <SeekBar
                android:id="@+id/slider1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/volume"
                android:min="0.0"
                android:max="1.0"
                android:layout_margin="0dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/selection_components"
                android:textSize="18sp"
                android:textColor="#666666"
                android:layout_margin="0dp"/>
            <com.google.android.material.tabs.TabLayout
                android:id="@+id/segment1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view_mode"
                app:items="List|Grid|Map"
                android:layout_margin="0dp"/>
            <RadioButton
                android:id="@+id/radio1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/select_size"
                app:items="Small|Medium|Large"
                android:layout_margin="0dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/loading_indicator"
                android:textSize="18sp"
                android:textColor="#666666"
                android:layout_margin="0dp"/>
            <View
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/loading"
                android:layout_margin="0dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/circle_image"
                android:textSize="18sp"
                android:textColor="#666666"
                android:layout_margin="0dp"/>
            <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_margin="0dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/gradient_view"
                android:textSize="18sp"
                android:textColor="#666666"
                android:layout_margin="0dp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="100dp"
                app:items="#FF6B6B|#4ECDC4"
                android:layout_margin="0dp">
                <com.kotlinjsonui.views.KjuiTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/gradient_background"
                    android:textSize="20sp"
                    android:textColor="#FFFFFF"
                    android:layout_centerInParent="true"/>
            </View>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/blur_view"
                android:textSize="18sp"
                android:textColor="#666666"
                android:layout_margin="0dp"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_margin="0dp">
                <com.kotlinjsonui.views.KjuiTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/blurred_background"
                    android:textSize="18sp"
                    android:textColor="#333333"
                    android:layout_centerInParent="true"/>
            </View>
        </LinearLayout>
    </ScrollView>
</layout>