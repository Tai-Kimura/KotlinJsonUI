<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <import type="android.view.View"/>
        <variable
            name="data"
            type="com.example.kotlinjsonui.sample.data.ConverterTestData"/>
        <variable
            name="viewModel"
            type="com.example.kotlinjsonui.sample.viewmodels.ConverterTestViewModel"/>
    </data>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:textColor="@color/white"
                android:text="@{data.dynamicModeStatus}"
                android:onClick="toggleDynamicMode"
                android:background="@drawable/ripple_d5af3557"
                android:textSize="14sp"
                android:textStyle="bold"
                android:padding="8dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@{data.title}"
                android:textSize="24sp"
                android:textColor="@color/black"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/converter_test_gradientview_test"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="10dp"/>
            <com.kotlinjsonui.views.KjuiGradientView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                app:gradientColors="#FF0000|#00FF00|#0000FF">
                <com.kotlinjsonui.views.KjuiTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/converter_test_diagonal_gradient"
                    android:textSize="16sp"
                    android:textColor="@color/white"
                    android:layout_centerInParent="true"/>
            </com.kotlinjsonui.views.KjuiGradientView>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/converter_test_blurview_test"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_marginTop="10dp"
                android:background="@color/medium_green_2">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">
                    <com.kotlinjsonui.views.KjuiTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/converter_test_background_text"
                        android:textSize="24sp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                    <com.kotlinjsonui.views.KjuiTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/converter_test_this_will_be_blurred"
                        android:textSize="16sp"
                        android:textColor="@color/light_orange"
                        android:layout_marginTop="50dp"
                        android:layout_marginStart="20dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <com.kotlinjsonui.views.KjuiBlurView
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:blurRadius="20.0"
                    android:background="@color/black_2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent">
                    <com.kotlinjsonui.views.KjuiTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/converter_test_clear_text_on_blur_layer"
                        android:textSize="18sp"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:layout_centerInParent="true"/>
                </com.kotlinjsonui.views.KjuiBlurView>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/converter_test_webview_test"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <WebView
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginTop="10dp"
                app:url="https://www.example.com"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/converter_test_tabview_test"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <com.google.android.material.tabs.TabLayout
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginTop="10dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/converter_test_collection_test_2"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <androidx.recyclerview.widget.RecyclerView
                android:layout_width="match_parent"
                android:layout_height="300dp"
                android:layout_marginTop="10dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/converter_test_image_test"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginTop="10dp"
                app:defaultImage="@drawable/exclamationmark"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/converter_test_networkimage_test"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiNetworkImageView
                android:layout_width="200dp"
                android:layout_height="150dp"
                android:layout_marginTop="10dp"
                app:url="https://picsum.photos/400/300"
                app:defaultImage="@drawable/photo"/>
        </LinearLayout>
    </ScrollView>
</layout>