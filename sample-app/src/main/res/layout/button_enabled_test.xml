<?xml version="1.0" encoding="UTF-8"?>
<!-- Generated from button_enabled_test.json with Data Binding -->
<!-- DO NOT EDIT MANUALLY - Use 'kjui generate' to update -->
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <import type="android.view.View"/>
        <variable
            name="data"
            type="com.example.kotlinjsonui.sample.data.ButtonEnabledTestData"/>
        <variable
            name="viewModel"
            type="com.example.kotlinjsonui.sample.viewmodels.ButtonEnabledTestViewModel"/>
    </data>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#FFFFFF"
        android:padding="20dp">
        <com.kotlinjsonui.views.KjuiButton
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:textColor="#FFFFFF"
            android:text="@string/dynamic_dynamicmodestatus"
            android:onClick="toggleDynamicMode"
            android:background="@drawable/ripple_b39393a0"
            android:textSize="14sp"
            android:textStyle="bold"
            android:padding="8dp"/>
        <com.kotlinjsonui.views.KjuiTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@{data.title}"
            android:textSize="24sp"
            android:textColor="#000000"
            android:layout_marginBottom="20dp"/>
        <com.kotlinjsonui.views.KjuiTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/button_enabled_state_isbuttonen"
            android:textSize="16sp"
            android:textColor="#666666"
            android:layout_marginBottom="10dp"/>
        <com.kotlinjsonui.views.KjuiButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/test_button_controlled_by_data"
            android:enabled="@{data.isButtonEnabled}"
            android:onClick="testAction"
            android:background="@drawable/ripple_6370d6db"
            android:textColor="#FFFFFF"
            android:padding="10dp"
            android:layout_marginBottom="20dp"/>
        <com.kotlinjsonui.views.KjuiButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/toggle_enabled_state"
            android:onClick="toggleEnabled"
            android:background="@drawable/ripple_14d5be80"
            android:textColor="#FFFFFF"
            android:padding="10dp"
            android:layout_marginBottom="10dp"/>
        <com.kotlinjsonui.views.KjuiButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/always_disabled_button"
            android:enabled="false"
            android:onClick="neverCalled"
            android:background="@drawable/ripple_10bcd0bb"
            android:textColor="#FFFFFF"
            android:padding="10dp"
            android:layout_marginBottom="10dp"/>
        <com.kotlinjsonui.views.KjuiButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/always_enabled_button"
            android:enabled="true"
            android:onClick="alwaysCalled"
            android:background="@drawable/ripple_2b95b0eb"
            android:textColor="#FFFFFF"
            android:padding="10dp"/>
    </LinearLayout>
</layout>