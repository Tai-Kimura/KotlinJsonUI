<?xml version="1.0" encoding="UTF-8"?>
<!-- Generated from binding_test.json with Data Binding -->
<!-- DO NOT EDIT MANUALLY - Use 'kjui generate' to update -->
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <import type="android.view.View"/>
        <variable
            name="data"
            type="com.example.kotlinjsonui.sample.data.BindingTestData"/>
        <variable
            name="viewModel"
            type="com.example.kotlinjsonui.sample.viewmodels.BindingTestViewModel"/>
    </data>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:textColor="@color/white"
                android:text="@{data.dynamicModeStatus}"
                android:onClick="toggleDynamicMode"
                android:background="@drawable/ripple_d5af3557"
                android:textSize="14sp"
                android:textStyle="bold"
                android:padding="8dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:id="@+id/title_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@{data.title}"
                android:textSize="24sp"
                android:textColor="@color/black"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_text_binding"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"
                android:textStyle="bold"/>
            <com.kotlinjsonui.views.KjuiEditText
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:hint="@string/binding_test_enter_text"
                android:text="@{data.textValue}"
                android:background="@drawable/shape_9a37b16d"
                android:padding="10dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_you_typed_textvalue"
                android:textSize="14sp"
                android:textColor="@color/medium_gray_4"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_counter_binding"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="30dp"
                android:textStyle="bold"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp">
                <com.kotlinjsonui.views.KjuiButton
                    android:layout_width="0dp"
                    android:layout_height="44dp"
                    android:layout_weight="1"
                    android:text="@string/binding_test_decrease"
                    android:onClick="decreaseCounter"
                    android:background="@drawable/ripple_03325df4"
                    android:textColor="@color/white"
                    android:layout_marginEnd="5dp"/>
                <com.kotlinjsonui.views.KjuiTextView
                    android:layout_width="100dp"
                    android:layout_height="44dp"
                    android:text="@{data.counter}"
                    android:textSize="20sp"
                    android:textColor="@color/black"
                    android:textAlignment="center"
                    android:background="@drawable/shape_0cd10af4"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="5dp"/>
                <com.kotlinjsonui.views.KjuiButton
                    android:layout_width="0dp"
                    android:layout_height="44dp"
                    android:layout_weight="1"
                    android:text="@string/binding_test_increase"
                    android:onClick="increaseCounter"
                    android:background="@drawable/ripple_1a0cce85"
                    android:textColor="@color/white"
                    android:layout_marginStart="5dp"/>
            </LinearLayout>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_toggle_binding"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="30dp"
                android:textStyle="bold"/>
            <Switch
                android:id="@+id/toggle_switch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:checked="@{data.toggleValue}"
                android:onClick="toggleChanged"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_toggle_is_togglevalue"
                android:textSize="14sp"
                android:textColor="@color/medium_gray_4"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/shape_b66efd58">
                <com.kotlinjsonui.views.KjuiTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/binding_test_onoff"
                    android:textSize="16sp"
                    android:textColor="@color/white"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_slider_binding"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="30dp"
                android:textStyle="bold"/>
            <SeekBar
                android:id="@+id/value_slider"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:min="0"
                android:max="100"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_slider_value_slidervalue"
                android:textSize="14sp"
                android:textColor="@color/medium_gray_4"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"/>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/shape_6c228cc3">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_bf88c147"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_selectbox_binding"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="30dp"
                android:textStyle="bold"/>
            <com.kotlinjsonui.views.KjuiSelectBox
                android:id="@+id/option_select"
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                app:selectedValue="@{data.selectedOption}"
                app:items="Option 1|Option 2|Option 3|Option 4|Option 5|Option 6|Option 7|Option 8|Option 9|Option 10|Option 11|Option 12|Option 13|Option 14|Option 15|Option 16|Option 17|Option 18|Option 19|Option 20|Option 21|Option 22|Option 23|Option 24|Option 25|Option 26|Option 27|Option 28|Option 29|Option 30"
                android:background="@drawable/shape_f7666c4d"
                android:textColor="@color/black"
                app:hintColor="@color/light_gray_8"
                android:hint="&quot;選択してください&quot;"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_selected_selectedoption"
                android:textSize="14sp"
                android:textColor="@color/medium_gray_4"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_date_picker_wheels_style"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:textStyle="bold"
                android:layout_marginTop="30dp"/>
            <com.kotlinjsonui.views.KjuiSelectBox
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                tools:selectItemType="Date"
                app:selectedValue="@{data.selectedDate}"
                app:datePickerMode="wheels"
                app:dateFormat="yyyy年MM月dd日"
                app:minDate="2020-01-01"
                app:maxDate="2030-12-31"
                android:hint="&quot;日付を選択&quot;"
                android:background="@drawable/shape_f7666c4d"
                android:textColor="@color/black"
                app:hintColor="@color/light_gray_8"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/binding_test_date_picker_compact_style"
                android:textSize="18sp"
                android:textColor="@color/dark_gray"
                android:textStyle="bold"
                android:layout_marginTop="30dp"/>
            <com.kotlinjsonui.views.KjuiSelectBox
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                tools:selectItemType="Date"
                app:selectedValue="@{data.selectedDate2}"
                app:datePickerMode="compact"
                app:dateFormat="MM/dd/yyyy"
                android:hint="@string/binding_test_select_date"
                android:background="@drawable/shape_f7666c4d"
                android:textColor="@color/black"
                app:hintColor="@color/light_gray_8"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@{data.selectedDate}"
                android:textSize="14sp"
                android:textColor="@color/medium_gray_4"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginBottom="30dp"/>
        </LinearLayout>
    </ScrollView>
</layout>