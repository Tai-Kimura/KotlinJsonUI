<?xml version="1.0" encoding="UTF-8"?>
<!-- Generated from disabled_test.json with Data Binding -->
<!-- DO NOT EDIT MANUALLY - Use 'kjui generate' to update -->
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <import type="android.view.View"/>
        <variable
            name="data"
            type="com.example.kotlinjsonui.sample.data.DisabledTestData"/>
        <variable
            name="viewModel"
            type="com.example.kotlinjsonui.sample.viewmodels.DisabledTestViewModel"/>
    </data>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:textColor="@color/white"
                android:text="@{data.dynamicModeStatus}"
                android:onClick="toggleDynamicMode"
                android:background="@drawable/ripple_d5af3557"
                android:textSize="14sp"
                android:textStyle="bold"
                android:padding="8dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:id="@+id/title_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@{data.title}"
                android:textSize="24sp"
                android:textColor="@color/black"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/disabled_test_enabled_button"
                android:textSize="16sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/disabled_test_enabled_button"
                android:background="@drawable/ripple_12c68881"
                android:textColor="@color/white"
                android:enabled="true"
                android:onClick="onEnabledButtonTap"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/disabled_test_disabled_button"
                android:textSize="16sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/disabled_test_disabled_button"
                android:background="@drawable/selector_d702e187"
                android:textColor="@color/white"
                android:enabled="false"
                android:onClick="onDisabledButtonTap"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/disabled_test_touchdisabledstate_button"
                android:textSize="16sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/disabled_test_touch_disabled"
                android:background="@drawable/ripple_abf4fa5f"
                android:textColor="@color/white"
                android:onClick="onTouchDisabledTap"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/disabled_test_enabled_textfield"
                android:textSize="16sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiEditText
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:hint="@string/disabled_test_enabled_can_type_here"
                android:text="@{data.textFieldValue}"
                android:background="@drawable/shape_9a37b16d"
                android:padding="10dp"
                android:enabled="true"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/disabled_test_disabled_textfield"
                android:textSize="16sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiEditText
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:hint="@string/disabled_test_disabled_cannot_type"
                android:text="@string/disabled_test_disabled_text_field"
                android:background="@drawable/selector_cd00cfc3"
                android:padding="10dp"
                android:enabled="false"
                android:textColor="@color/medium_gray_4"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/disabled_test_dynamic_enabledisable_test"
                android:textSize="16sp"
                android:textColor="@color/dark_gray"
                android:layout_marginTop="30dp"
                android:textStyle="bold"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/disabled_test_toggle_enable_state"
                android:background="@drawable/ripple_1a0cce85"
                android:textColor="@color/white"
                android:onClick="toggleEnableState"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/disabled_test_dynamic_button"
                android:background="@drawable/selector_27c8522a"
                android:textColor="@color/white"
                android:enabled="@{data.isEnabled}"
                android:onClick="onDynamicButtonTap"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/disabled_test_current_state_isenabled"
                android:textSize="14sp"
                android:textColor="@color/medium_gray_4"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"/>
        </LinearLayout>
    </ScrollView>
</layout>