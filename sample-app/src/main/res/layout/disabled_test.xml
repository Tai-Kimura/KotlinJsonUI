<?xml version="1.0" encoding="UTF-8"?>
<!-- Generated from disabled_test.json with Data Binding -->
<!-- DO NOT EDIT MANUALLY - Use 'kjui generate' to update -->
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <import type="android.view.View"/>
        <variable
            name="data"
            type="com.example.kotlinjsonui.sample.data.DisabledTestData"/>
        <variable
            name="viewModel"
            type="com.example.kotlinjsonui.sample.viewmodels.DisabledTestViewModel"/>
    </data>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="wrap_content"
                android:layout_height="44dp"
                android:textColor="#FFFFFF"
                android:text="@string/dynamic_dynamicmodestatus"
                android:onClick="toggleDynamicMode"
                android:background="@drawable/ripple_b39393a0"
                android:textSize="14sp"
                android:textStyle="bold"
                android:padding="8dp"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:id="@+id/title_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@{data.title}"
                android:textSize="24sp"
                android:textColor="#000000"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/enabled_button"
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/enabled_button"
                android:background="@drawable/ripple_56a40756"
                android:textColor="#FFFFFF"
                android:enabled="true"
                android:onClick="onEnabledButtonTap"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/disabled_button"
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/disabled_button"
                android:background="@drawable/selector_2aad8b20"
                android:textColor="#FFFFFF"
                android:enabled="false"
                android:onClick="onDisabledButtonTap"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/touchdisabledstate_button"
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/touch_disabled"
                android:background="@drawable/ripple_4c192b00"
                android:textColor="#FFFFFF"
                android:onClick="onTouchDisabledTap"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/enabled_textfield"
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiEditText
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:hint="@string/enabled_can_type_here"
                android:text="@{data.textFieldValue}"
                android:background="@drawable/shape_54109721"
                android:padding="10dp"
                android:enabled="true"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/disabled_textfield"
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_marginTop="20dp"/>
            <com.kotlinjsonui.views.KjuiEditText
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:hint="@string/disabled_cannot_type"
                android:text="@string/disabled_text_field"
                android:background="@drawable/selector_db1e7661"
                android:padding="10dp"
                android:enabled="false"
                android:textColor="#666666"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/dynamic_enabledisable_test"
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_marginTop="30dp"
                android:textStyle="bold"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/toggle_enable_state"
                android:background="@drawable/ripple_d7b81a7f"
                android:textColor="#FFFFFF"
                android:onClick="toggleEnableState"/>
            <com.kotlinjsonui.views.KjuiButton
                android:layout_width="match_parent"
                android:layout_height="44dp"
                android:layout_marginTop="10dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="@string/dynamic_button"
                android:background="@drawable/selector_eef4483b"
                android:textColor="#FFFFFF"
                android:enabled="@{data.isEnabled}"
                android:onClick="onDynamicButtonTap"/>
            <com.kotlinjsonui.views.KjuiTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/current_state_isenabled"
                android:textSize="14sp"
                android:textColor="#666666"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal"/>
        </LinearLayout>
    </ScrollView>
</layout>